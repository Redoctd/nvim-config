{
  "function": {
    "prefix": "fun",
    "body": ["fun ${1}(${2}): ${3} {", "    ${4}", "}"]
  },
  "data class": {
    "prefix": "data",
    "body": ["data class ${1}(${2}: ${3})"]
  },
  "if expression": {
    "prefix": "if",
    "body": ["if (${1}) {", "    ${2}", "} else {", "    ${3}", "}"]
  },
  "when expression": {
    "prefix": "when",
    "body": [
      "when (${1}) {",
      "    ${2}",
      "    else -> {",
      "        ${3}",
      "    }",
      "}"
    ]
  },
  "try-catch": {
    "prefix": "try",
    "body": [
      "try {",
      "    ${1}",
      "} catch (e: ${2}Exception) {",
      "    ${3}",
      "}"
    ]
  },
  "for loop": {
    "prefix": "for",
    "body": ["for (${1} in ${2}) {", "    ${3}", "}"]
  },
  "while loop": {
    "prefix": "while",
    "body": ["while (${1}) {", "    ${2}", "}"]
  },
  "do-while loop": {
    "prefix": "do",
    "body": ["do {", "    ${1}", "} while (${2})"]
  },
  "null check": {
    "prefix": "null",
    "body": ["if (${1} == null) {", "    ${2}", "}"]
  },
  "lambda": {
    "prefix": "lambda",
    "body": ["val ${1} = { ${2}: ${3} -> ${4} }"]
  }
}
